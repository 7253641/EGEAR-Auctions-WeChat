.live-header .live-header-left{
	display: inline-block;
	width: 650rpx;
	line-height: 40rpx;
}
.live-header .live-header-left view:first-of-type{
	font-size: 24rpx;
	color: #fff;
}
.live-header .live-header-left view:last-of-type{
	font-size: 24rpx;
	color: #fff;
}
.live-header .live-header-left view:last-of-type text::before{
	content: attr(data-title);
}
.live-header .live-header-left view:first-of-type text .blue{
	font-size: 32rpx;
}
.live-header .live-header-left view:first-of-type text:last-of-type{
	margin-left: 20rpx;
}
.live-header .live-header-right{
	position: absolute;
	right: 30rpx;
	bottom: 15rpx;
	border: 1rpx solid #fff;
	padding: 0rpx 10rpx;
	border-radius: 20rpx;
	line-height: 35rpx;
	font-size: 20rpx;
	color: #fff;
}
/* tabs */
.tabs{
	width: 750rpx;
	height: 120rpx;
	padding: 30rpx 30rpx 0 30rpx;
	background-color: #fff;
	position: fixed;
	top: 424rpx;
	left: 0;
	z-index: 998;
}
.tabs view{
	width: 690rpx;
	height: 85rpx;
	border: 1rpx solid #EEEEEE;
	border-radius: 42rpx;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.tabs view text{
	width: 214rpx;
	height: 68rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	border-radius: 34rpx;
	color: #999;
	font-size: 32rpx;
	font-weight: 500;
}
.tabs view .tabs-choose{
	background: linear-gradient( 90deg,rgba(4, 145, 215, 1) 0%,rgba(53, 82, 255, 1) 0%,rgba(4, 145, 215, 1) 100%,rgba(0, 64, 253, 1) 100%);
	color: #fff;
}

/* public-header */
.public-header{
	width: 690rpx;
	font-size: 32rpx;
	color: #333;
	font-weight: 500;
	padding: 40rpx 0 20rpx 0;
	margin-left: 30rpx;
	display: flex;
	justify-content: flex-start;
	align-items: center;
}
.public-header::before{
	content: '';
	width: 20rpx;
	height: 32rpx;
	border-left: 6rpx solid #1F6FED;
}
.public-block{
	width: 750rpx;
	padding-bottom: 30rpx;
}
.public-view{
	width: 750rpx;
	height: 544rpx;
}
/* 提示 */
.prompt{
	width: 750rpx;
	height: 60rpx;
	background-color: #F0F0F5;
	box-sizing: border-box;
	padding: 0 30rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-top: 10rpx;
}
.prompt:first-of-type{
	margin-top: 20rpx;
}
.prompt view{
	height: 60rpx;
	display: flex;
	align-items: center;
}
.prompt view text{
	font-size: 22rpx;
	color: #666;
}
.prompt view navigator{
	font-size: 22rpx;
	color: #1F6FED;
	margin-left: 20rpx;
}
.prompt image{
	width: 28rpx;
	height: 28rpx;
}
/* 拍卖会信息 */
.auction-info{
    width: 690rpx;
    margin-left: 30rpx;
    padding: 30rpx;
    border: 1rpx solid #eee;
    border-radius: 5rpx;
}
.auction-info view{
    display: flex;
    height: 60rpx;
    color: #333;
    align-items: center;
}
.auction-info view text:first-of-type{
    font-size: 28rpx;
    color: #999;
}
.auction-info view text:last-of-type{
    font-size: 30rpx;
}
.auction-info view .auction-info-bl{
    color: #1F6FED;
}
/* 竞价规则 */
.rule{
	width: 750rpx;
}
.rule .rule-ex{
	width: 690rpx;
	background-color: #F0F0F5;
	border-radius: 7rpx;
	padding: 26rpx 20rpx;
	margin-left: 30rpx;
}
.rule .rule-ex text{
	font-size: 24rpx;
	font-weight: 400;
	color: #666;
	line-height: 34rpx;
}
.rule .rule-person{
	width: 690rpx;
	margin-bottom: 30rpx;
	box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(0, 0, 0, 0.1);
	border-radius: 4rpx;
	display: flex;
	justify-content: space-between;
	margin-left: 30rpx;
}
.rule .rule-person .rule-person-left{
	margin: 30rpx;
	width: 176rpx;
}
.rule .rule-person .rule-person-right{
	margin: 30rpx 30rpx 30rpx 0;
	margin-right: 30rpx;
	width: 424rpx;
}
.rule .rule-person image{
	width: 176rpx;
	height: 176rpx;
	background-color: aliceblue;
	border-radius: 88rpx;
}
.rule .rule-person .rule-person_num{
	display: block;
	font-size: 20rpx;
	color: #999;
	line-height: 28rpx;
	text-align: center;
}
.rule .rule-person .rule-person_name{
	font-size: 32rpx;
	color: #333;
}
.rule .rule-person .rule-person_state{
	margin-left: 20rpx;
	font-size: 20rpx;
	color: #1F6FED;
	border: 1rpx solid #1F6FED;
	border-radius: 20rpx;
	padding: 7rpx 20rpx;
}
.rule .rule-person .rule-person_des{
	display: block;
	margin-top: 24rpx;
	font-size: 26rpx;
	color: #666;
	line-height: 36rpx;
}
/* 竞拍列表 */
.list{
	width: 690rpx;
}
.list-view{
	width: 690rpx;
	height: 220rpx;
	border-bottom: 1rpx solid rgba(0, 0, 0, .1);
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-left: 30rpx;
}
.list-view .list-view-left{
	width: 260rpx;
	height: 152rpx;
	display: flex;
	justify-content: space-between;
}
.list-view .list-view-left .list-view-left-num{
	width: 108rpx;
	height: 152rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.list-view .list-view-left .list-view-left-num text{
	font-size: 32rpx;
	color: #333;
	font-weight: 500;
}
.list-view .list-view-left .list-view-left-img{
	width: 152rpx;
	height: 152rpx;
	position: relative;
}
.list-view .list-view-left .list-view-left-img image{
	width: 152rpx;
	height: 152rpx;
}
.list-view .list-view-left .list-view-left-state-ing{
	position: absolute;
	width: 74rpx;
	height: 40rpx;
	background: linear-gradient(270deg,rgba(253,58,117,1) 0%,rgba(249,51,51,1) 100%);
	border-radius: 0rpx 0rpx 20rpx 0rpx;
	text-align: center;
	line-height: 40rpx;
	color: #fff;
	font-size: 20rpx;
	top: 0;
	left: 0;
}
.list-view .list-view-left .list-view-left-state-success{
	position: absolute;
	width: 74rpx;
	height: 40rpx;
	background: linear-gradient(90deg,rgba(86,145,200,1) 0%,rgba(53,82,255,1) 0%,rgba(4,145,215,1) 100%,rgba(0,64,253,1) 100%);
	border-radius: 0rpx 0rpx 20rpx 0rpx;
	text-align: center;
	line-height: 40rpx;
	color: #fff;
	font-size: 20rpx;
	top: 0;
	left: 0;
}
.list-view .list-view-left .list-view-left-state-end{
	position: absolute;
	width: 152rpx;
	height: 152rpx;
	background: rgba(0, 0, 0, .5);
	text-align: center;
	line-height: 152rpx;
	color: #fff;
	font-size: 32rpx;
	top: 0;
	left: 0;
}
.list-view .list-view-right{
	width: 410rpx;
	height: 152rpx;
	position: relative;
}
.list-view .list-view-right .list-view_name{
	display: block;
	width: 410rpx;
	overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
	font-size: 32rpx;
	font-weight: 500;
	color: #333;
	line-height: 44rpx;
}
.list-view .list-view-right .list-view_prems{
	display: block;
	width: 410rpx;
	font-size: 26rpx;
	color: #999;
	overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
	margin-top: 16rpx;
}
.list-view .list-view-right .list-view_state{
	position: absolute;
	left: 0;
	bottom: 0;
	background:rgba(31, 111, 237, .1);
	border-radius: 2rpx;
	font-size: 24rpx;
	color: #1F6FED;
	padding: 5rpx 10rpx;
}
.list-view .list-view-right .list-view_price{
	position: absolute;
	left: 136rpx;
	bottom: 0;
	font-size: 28rpx;
	color: #1F6FED;
	font-weight: 500;
}
.list-view .list-view-right .list-view_price::before{
	content: attr(data-txt);
	font-size: 26rpx;
	color: #666;
	font-weight: 400;
}
.list-view .list-view-right .list-view_now{
	position: absolute;
	left: 0;
	bottom: 0;
	font-size: 26rpx;
	color: #666;
}
.list-view .list-view-right .list-view_now .lar{
	display: inline-block;
	height: 40rpx;
	font-size: 40rpx;
	color: #1f6fed;
	font-weight: 500;
}
.list-view .list-view-right .list-view_num{
	position: absolute;
	right: 0;
	bottom: 0;
	font-size: 26rpx;
	color: #666;
}
.list-view .list-view-right .list-view_num .lar{
	display: inline-block;
	height: 40rpx;
	font-size: 40rpx;
	color: #1f6fed;
	font-weight: 500;
}
.border-none{
	border: none;
}
/* 竞价大厅 */
.hall{
	width: 750rpx;
}
	/* ===================
	*	当前拍品
	===================== */
.hall .list-view{
	height: 152rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.hall .padding-view{
	margin: 10rpx 0;
}
	/* ===================
	*	出价记录
	===================== */
.hall .hall-list{
	width: 690rpx;
	background-color: #F0F0F5;
	border-radius: 7rpx;
	padding: 20rpx;
	/* overflow-y: scroll;
  	overflow: hidden scroll; */
	margin-left: 30rpx;
}
.hall .hall-list .hall-list-block{
	display: flex;
	height: 60rpx;
	justify-content: space-around;
}
.hall .hall-list .hall-list-block text{
	font-size: 24rpx;
	color: #666;
}
.hall .hall-list .hall-list-block .lar{
	font-size: 24rpx;
	color: #1f6fed;
	font-weight: 500;
}
.hall .hall-list .hall-list-nodata{
	display: flex;
	justify-content: center;
}
.hall .hall-list .hall-list-nodata text{
	font-size: 24rpx;
	color: #666;
}
	/* ===================
	*	快速出价
	===================== */
.hall .hall_{
	position: fixed;
	bottom: 0;
	left: 0;
	width: 750rpx;
	height: 380rpx;
	background-color: #fff;
	border-top: 1rpx solid #eee;
}
.hall .hall_ .public-header_fix{
	display: block;
	font-size: 32rpx;
	color: #333;
	font-weight: 500;
	line-height: 80rpx;
	margin-left: 30rpx;
}
.hall .hall_ .hall-tabs{
	width: 750rpx;
	height: 120rpx;
	box-sizing: border-box;
	padding: 0 30rpx;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.hall .hall_ .hall-tabs text{
	width: 160rpx;
	height: 60rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	border: 1rpx solid #dbdbdb;
	font-size: 28rpx;
	color: #666;
	border-radius: 4rpx;
}
.hall .hall_ .hall-tabs .priceBlue{
	border: 1rpx solid #1F6FED;
	color: #1F6FED;
}
.hall .hall_ .hall-price{
	width: 750rpx;
	height: 80rpx;
	box-sizing: border-box;
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 0 30rpx;
}
.hall .hall_ .hall-price text{
	font-size: 30rpx;
	color: #666;
}
.hall .hall_ .hall-price text .lar{
	font-size: 34rpx;
	color: #1f6fed;
	font-weight: 500;
}
.hall .hall_ .hall-btn{
	position: fixed;
	bottom: 0;
	right: 0;
	width: 750rpx;
	height: 100rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	color: #fff;
	font-size: 36rpx;
}
.hall .hall_ .gra{
	background-color: #eee;
}
.hall .hall_ .bl{
	background: linear-gradient(90deg,rgba(86,145,200,1) 0%,rgba(53,82,255,1) 0%,rgba(4,145,215,1) 100%,rgba(0,64,253,1) 100%);
}
/* ===================
*	蒙板
===================== */
.hall-layer{
	width: 750rpx;
	background-color: #FFF;
	position: fixed;
	bottom: 0;
	left: 0;
	opacity: .8;
	display: flex;
	flex-flow: column;
    justify-content: flex-end;
    align-items: center;
}
.hall-layer view{
    width: 750rpx;
    height: 50rpx;
	display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 70rpx;
}
.hall-layer view image{
    width: 40rpx;
    height: 50rpx;
}
.hall-layer view text{
    font-size: 36rpx;
    color: #333;
    margin-left: 20rpx;
}
.hall-layer .hall-layer-bottom{
    width: 400rpx;
    height: 90rpx;
    background: linear-gradient(90deg,rgba(86,145,200,1) 0%,rgba(53,82,255,1) 0%,rgba(4,145,215,1) 100%,rgba(0,64,253,1) 100%);
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(31,111,237,1);
    border-radius: 45rpx;
    font-size: 36rpx;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 150rpx;
}
/* 所有设备处理中请稍后... */
.hall-nodata{
	width: 750rpx;
	display: flex;
	justify-content: center;
	margin-top: 100rpx;
}
.hall-nodata text{
    font-size: 32rpx;
    color: #999;
}
/* 底部填充图层 */
.bottomblock{
	width: 750rpx;
	height: 380rpx;
}
/* 拍卖师喊话 */
.shout{
	position: fixed;
	width: 750rpx;
	right: 10rpx;
	bottom: 400rpx;
	display: flex;
	align-items: flex-end;
	flex-flow: column;
}
.shout view{
	height: 90rpx;
	margin-top: 30rpx;
	border-radius: 10rpx;
	background-color: #f8b62d;
	padding: 0 50rpx;
	display: flex;
	flex-flow: nowrap;
	justify-content: space-between;
	align-items: center;
	position: relative;
}
.shout view::after{
	content: "";
	position: absolute;
	width: 30rpx;
	height: 30rpx;
	background-color: #f8b62d;
	bottom: -10rpx;
	right: 30rpx;
	transform: rotate(45deg);
}
.shout view image{
	width: 32rpx;
	height: 32rpx;
}
.shout view text{
	font-size: 30rpx;
	color: #fff;
	font-weight: 500;
	margin-left: 20rpx;
}
.shoutTest{
	position: fixed;
	width: 300rpx;
	height: 80rpx;
	background-color: red;
	font-size: 30rpx;
	color: #fff;
	left: 30rpx;
	bottom: 600rpx;
	border-radius: 25rpx;
	text-align: center;
	line-height: 80rpx;
}