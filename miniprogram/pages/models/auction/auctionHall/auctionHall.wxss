/* auctionHall.wxss */
page{
	background-color: #fff;
}
.live{
	width: 750rpx;
	height: 424rpx;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 998;
}
.live-header{
	width: 750rpx;
	height: 100rpx;
	position: fixed;
	background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 1) 100%);
	top: 324rpx;
	left: 0;
	z-index: 999;
	padding: 0 30rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
